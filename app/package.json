{
  "name": "typasaur",
  "version": "1.0.0",
  "description": "Convert JSON to TypeScript types/interfaces from the CLI",
  "type": "commonjs",
  "main": "src/core.js",
  "bin": {
    "typasaur": "typasaur.js"
  },
  "keywords": [
    "typescript",
    "cli",
    "json",
    "codegen",
    "types",
    "interfaces"
  ],
  "license": "MIT",
  "author": "Kevin Gleeson",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KevinG115/typasaur.git"
  },
  "bugs": {
    "url": "https://github.com/KevinG115/typasaur/issues"
  },
  "homepage": "https://github.com/KevinG115/typasaur#readme",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "app/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node src/typasaur.js",
    "prepublishOnly": "node -e \"require('fs').accessSync('typasaur.js'); console.log('CLI entry ok')\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "prepare": "husky install"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "dependencies": {
    "posthog-node": "^5.7.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "jest": "^30.0.5"
  }
}
